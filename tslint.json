{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "no-implicit-dependencies": false,
    "prettier": true
  },
  "rulesDirectory": ["tslint-plugin-prettier"],
  "exclude": ["node_modules"],
  "autoFixOnSave": true,
  "linterOptions": {
    "exclude": ["node_modules/**", "build/**", "**/*.js"]
  }
}
